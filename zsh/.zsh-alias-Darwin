qr() {
    content="$1"
    if [ -z "$content" ]; then
        content="$(pbpaste)"
    fi
    if [ -n "$content" ]; then
        # qrencode -t UTF8 "$content"
        qrencode -t png "$content" -o - -s 6 | open -a Preview.app -f
    else
        echo empty input
    fi
}

free() {
  ps -caxm -orss= | awk '{ sum += $1 } END { print "rss: " sum/1024/1024 " G" }'
}